	<script src="<%- config.root %>js/jquery.fancybox.js"></script>
	<script type="text/javascript">
		$(document).ready(function() {
			$(".fancybox").fancybox({
	            padding: 0,
	            helpers: {
	                overlay: {
	                  locked: false
	                }
	            }
	        });
		});
	</script>
<% } %>